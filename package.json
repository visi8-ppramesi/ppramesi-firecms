{
    "name": "ppramesi-firecms",
    "version": "2.0.0-beta.2",
    "description": "FireCMS but less retarded",
    "author": "camberi",
    "license": "MIT",
    "repository": "git@github.com:Camberi/firecms.git",
    "main": "./dist/index.umd.js",
    "module": "./dist/index.es.js",
    "types": "dist/index.d.ts",
    "source": "src/index.ts",
    "engines": {
        "node": ">=14"
    },
    "exports": {
        ".": {
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js"
        }
    },
    "keywords": [
        "firebase",
        "cms",
        "admin",
        "admin panel",
        "firebase panel",
        "firestore",
        "headless",
        "headless cms",
        "content manager"
    ],
    "scripts": {
        "prepare": "npm run build",
        "build": "vite build && tsc --emitDeclarationOnly",
        "prepublishOnly": "run-s build",
        "createTag": "PACKAGE_VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags",
        "test": "run-s test:unit test:lint",
        "test:lint": "eslint \"src/**\" --quiet",
        "test:unit": "cross-env CI=1 react-scripts test --transformIgnorePatterns \"node_modules/(?!react-markdown)/\"",
        "test:navigation": "cross-env CI=1 react-scripts test navigation.test.ts --transformIgnorePatterns \"node_modules/(?!react-markdown)/\"",
        "test:watch": "react-scripts test --transformIgnorePatterns \"node_modules/(?!react-markdown)/\""
    },
    "dependencies": {
        "@date-io/date-fns": "^2.16.0",
        "@fontsource/ibm-plex-mono": "^4.5.13",
        "@hello-pangea/dnd": "^16.2.0",
        "date-fns": "^2.29.3",
        "formik": "^2.2.9",
        "history": "^5.3.0",
        "markdown-it": "^13.0.1",
        "notistack": "^2.0.8",
        "object-hash": "^3.0.0",
        "react-dropzone": "^14.2.3",
        "react-fast-compare": "^3.2.0",
        "react-image-file-resizer": "^0.4.8",
        "react-markdown-editor-lite": "^1.3.4",
        "react-transition-group": "^4.4.5",
        "react-use-measure": "^2.1.1",
        "react-window": "^1.8.8",
        "typeface-roboto": "^1.1.13",
        "typeface-rubik": "^1.1.13",
        "yup": "^0.32.11"
    },
    "peerDependencies": {
        "@emotion/react": "^11.9.3",
        "@emotion/styled": "^11.9.3",
        "@mui/icons-material": "^5.8.4",
        "@mui/lab": "^5.0.0-alpha.90",
        "@mui/material": "^5.9.0",
        "@mui/x-date-pickers": "^5.0.0-beta.1",
        "algoliasearch": "^4.13.0",
        "firebase": "^9.0.0",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-router": "^6.2.0",
        "react-router-dom": "^6.2.0"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "devDependencies": {
        "@emotion/react": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "@mui/icons-material": "^5.11.0",
        "@mui/lab": "^5.0.0-alpha.117",
        "@mui/material": "^5.11.6",
        "@mui/x-date-pickers": "^5.0.15",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/jest": "^29.4.0",
        "@types/node": "^18.11.18",
        "@types/object-hash": "^3.0.2",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "@types/react-measure": "^2.0.8",
        "@typescript-eslint/eslint-plugin": "^5.49.0",
        "@typescript-eslint/parser": "^5.49.0",
        "@vitejs/plugin-react": "^3.0.1",
        "algoliasearch": "^4.14.3",
        "cross-env": "^7.0.3",
        "eslint": "^8.32.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-n": "^15.6.1",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.32.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "firebase": "^9.16.0",
        "jest": "^29.4.0",
        "npm-run-all": "^4.1.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router": "^6.7.0",
        "react-router-dom": "^6.7.0",
        "tsd": "^0.25.0",
        "typescript": "^4.9.4",
        "vite": "^4.0.4"
    },
    "files": [
        "dist"
    ]
}
